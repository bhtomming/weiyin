{% for strade in value.owner.goodsDetail %}
    {% set product = strade.product %}
    <div class="panel-body">
    衣服编号:{% for no in  strade.tradeNo %}{{ no }}<br/>{% endfor %}
    衣服{{ product.title }}<br/>
    单价{{ product.price }}<br/>
    描述{{ product.description | raw }}<br/>
    数量{{ strade.number }}<br/>
    金额{{ strade.amount }}<br/>
    </div>
{% endfor %}
